{
  "name": "request-catcher",
  "version": "2.0.0",
  "description": "Request catcher - Capture all requests and save them to a unique bin",
  "main": "server.ts",
  "author": "Joost Nijhuis <jnijhuis81@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn prepare && yarn start:build_nodemon",
    "prepare": "rimraf dist && mkdir -p ./dist && cp -a ./src/i18n ./dist",
    "start:build_nodemon": "concurrently --names \"build,nodemon\" \"yarn build --watch\" \"yarn nodemon\"",
    "build": "yarn prepare && babel ./src --out-dir dist --extensions \".ts,.tsx,.js,.jsx\"",
    "nodemon": "wait-on dist/server.js && NODE_ENV=development nodemon dist/server.js",
    "tsc": "tsc --isolatedModules false",
    "lint": "tslint -c tslint.json --fix --format verbose  src/**/*.{ts,tsx}",
    "server": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "async-redis": "^2.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "ejs": "^3.1.7",
    "express": "^4.17.3",
    "express-session": "^1.16.2",
    "express-validation": "^3.0.8",
    "helmet": "^4.6.0",
    "i18next": "^21.3.3",
    "i18next-http-middleware": "^3.1.4",
    "merge": "^2.1.1",
    "moment": "^2.29.4",
    "morgan": "^1.9.1",
    "node": "^12.6.0",
    "redis": "^3.1.1",
    "regenerator": "^0.14.1",
    "rotating-file-stream": "^1.4.2",
    "socket.io": "^4.3.1",
    "socketio-wildcard": "^2.0.0",
    "uuidv4": "^6.2.12",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/i18next": "^13.0.0",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/redis": "^2.8.32",
    "@types/socketio-wildcard": "^2.0.4",
    "@types/uuidv4": "^5.0.0",
    "@types/xml": "^1.0.6",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6",
    "redoc-cli": "^0.13.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^4.4.4",
    "wait-on": "^3.2.0"
  }
}
